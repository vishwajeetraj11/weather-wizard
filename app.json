{
  "expo": {
    "name": "Weather Wizard",
    "slug": "weather-wizard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Simple Weather App. (Not Word-Friendly)",
    "githubUrl": "https://github.com/vishwajeetraj11/weather-wizard"
  }
}
